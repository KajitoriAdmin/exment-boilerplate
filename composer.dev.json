{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "dms/phpunit-arraysubset-asserts": "~0.1",
        "exceedone/exment": "dev-php8_laravel9",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/browser-kit-testing": "~6.2",
        "laravel/framework": "^9.19",
        "laravel/sanctum": "^2.14.1",
        "laravel/tinker": "^2.7",
        "laravel/ui": "^3.0",
        "symfony/css-selector": "~5.2"
    },
    "require-dev": {
        "facade/ignition": "^2.0",
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Exceedone\\Exment\\": "./exment/src",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "exment:test:init": [
            "Composer\\Config::disableProcessTimeout",
            "@php artisan exment:inittest --yes"
        ],
        "exment:test:browser": [
            "@composer run exment:test:init",
            "Composer\\Config::disableProcessTimeout",
            "phpunit ./vendor/exceedone/exment/tests/Browser"
        ],
        "exment:test:feature": [
            "@composer run exment:test:init",
            "Composer\\Config::disableProcessTimeout",
            "phpunit ./vendor/exceedone/exment/tests/Feature -d memory_limit=512M"
        ],
        "exment:test:unit": [
            "@composer run exment:test:init",
            "Composer\\Config::disableProcessTimeout",
            "phpunit ./vendor/exceedone/exment/tests/Unit"
        ],
        "exment:test": [
            "@composer run exment:test:init",
            "Composer\\Config::disableProcessTimeout",
            "phpunit ./vendor/exceedone/exment/tests"
        ]
    },
    "repositories": [
	{
            "type": "path",
            "url": "./exment",
            "options": {
                "symlink": true
            }
        }
    ]
}
