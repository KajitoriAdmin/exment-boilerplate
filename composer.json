{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "dms/phpunit-arraysubset-asserts": "~0.1",
        "exceedone/exment": "dev-master",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/browser-kit-testing": "~6.2",
        "laravel/framework": "^8.0",
        "laravel/ui": "^3.0",
        "laravel/tinker": "^2.5",
        "symfony/css-selector": "~5.2"
    },
    "require-dev": {
        "facade/ignition": "^2.0",
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "exment:test:init": [
            "Composer\\Config::disableProcessTimeout",
            "@php artisan exment:inittest --yes"
        ],
        "exment:test:browser": [
            "@composer run exment:test:init",
            "phpunit ./vendor/exceedone/exment/tests/Browser"
        ],
        "exment:test:feature": [
            "@composer run exment:test:init",
            "phpunit ./vendor/exceedone/exment/tests/Feature -d memory_limit=512M"
        ],
        "exment:test:unit": [
            "@composer run exment:test:init",
            "phpunit ./vendor/exceedone/exment/tests/Unit"
        ],
        "exment:test": [
            "@composer run exment:test:init",
            "phpunit ./vendor/exceedone/exment/tests"
        ]
    }
}
